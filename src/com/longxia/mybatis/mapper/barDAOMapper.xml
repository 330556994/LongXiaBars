<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace必须是接口的全类名  -->
<mapper namespace="com.longxia.mybatis.mapper.BarDAO">
	<select id="findAll" resultType="bar" >
		select * from lx_bar
	</select>
	
	<select id="findByCategory2Id" resultType="bar" parameterType="int">
		select * from lx_bar where categoryid = #{category2Id}
	</select>
	
	
</mapper>

